    "T = int(input())\n",
    "time_list = list(map(int, input().split()))\n",
    "Y = 0\n",
    "M = 0\n",
    "for time in time_list:\n",
    "    Y += 10 * (time//30 + bool(time%30))\n",
    "    M += 15 * (time//60 + bool(time%60))\n",
    "if Y < M:\n",
    "    print('Y', Y)\n",
    "elif Y > M:\n",
    "    print('M', M)\n",
    "else:\n",
    "    print('Y M', Y)"
   ]
